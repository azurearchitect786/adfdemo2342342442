{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "factory4654"
		},
		"cloudstaginglake657_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'cloudstaginglake657'"
		},
		"cloudstaginglake657_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cloudstaginglake657.dfs.core.windows.net/"
		},
		"githubserver_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/ammarasmro/mslearn-dp100/main/data/diabetes.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "factory4654_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"cloudstaginglake657_accountKey": {
						"value": "[parameters('cloudstaginglake657_accountKey')]"
					},
					"cloudstaginglake657_properties_typeProperties_url": {
						"value": "[parameters('cloudstaginglake657_properties_typeProperties_url')]"
					},
					"githubserver_properties_typeProperties_url": {
						"value": "[parameters('githubserver_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}